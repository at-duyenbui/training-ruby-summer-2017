<section class="wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h3 class="page-header"><i class="fa fa-table"></i> List users</h3>
    </div>
  </div>
  <div class="row">
      <div class="col-lg-12">
          <section class="panel">
            <header class="panel-heading">
              List
            </header>
            <table class="table table-striped table-advance table-hover">
            <tbody>
              <tr>
                 <th>User Name</th>
                 <th>Email</th>
                 <th>Action</th>
              </tr>
            </tbody>
            <%= render partial: "user", collection: @users %>
            </table>
            <p><%= link_to "Add new user", new_user_path %></p>
          </section>
      </div>
  </div>
</section>



