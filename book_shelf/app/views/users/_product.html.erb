<tr>
  <td><%= product.user_name %></td>
  <td><%= product.email %></td>
  <td><%= product.role %></td>
  <td><%= product.provider %></td>
  <td><%= product.uid %></td>
  <td>
    <div class="btn-group">
      <%= link_to "Detail", user_path(product) %> |
      <%= link_to "Edit", edit_user_path(product) %> |
      <%= link_to "Delete", user_path(product), method: :delete, data: {confirm: 'Are you sure?'} %>
    </div>
  </td>
</tr>