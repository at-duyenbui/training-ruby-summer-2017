<tr>
  <td><%= book['name'] %></td>
  <td><%= book['author'] %></td>
  <td><%= book['title'] %></td>
  <td><%= book['isn'] %></td>
  <td>
    <div class="btn-group">
      <%= link_to "Detail", book_path(book) %> |
      <%= link_to "Edit", edit_book_path(book) %> |
      <%= link_to "Delete", book_path(book), method: :delete, data: {confirm: 'Are you sure?'} %>
    </div>
  </td>
</tr>