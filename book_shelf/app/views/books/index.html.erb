<section class="wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h3 class="page-header"><i class="fa fa-table"></i> List books</h3>
    </div>
  </div>
  <div class="row">
      <div class="col-lg-12">
          <section class="panel">
            <header class="panel-heading">
              List
            </header>
            <table class="table table-striped table-advance table-hover">
            <tbody>
              <tr>
                 <th>Book's Name</th>
                 <th>Author</th>
                 <th>Title</th>
                 <th>Isn</th>
                 <th>Action</th>
              </tr>
              <%= render partial: "book", collection: @books %>
            </tbody>
            </table>
            <p><%= link_to "Add new book", new_book_path %></p>
          </section>
      </div>
  </div>
</section>